<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?impoet View.*?>

<Pane fx:id="pane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="900.0" stylesheets="@S.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.GameOptionsController">
   <children>
      <ImageView fx:id="background" fitHeight="514.0" fitWidth="920.0" layoutX="-7.0" pickOnBounds="true" preserveRatio="false">
      </ImageView>
      <Button fx:id="easy_game" layoutX="80.0" layoutY="70.0" mnemonicParsing="false" onAction="#Easy_Game_Click" prefHeight="68.0" prefWidth="155.0" text="Easy">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button fx:id="medium_game" layoutX="380.0" layoutY="70.0" mnemonicParsing="false" onAction="#Medium_Game_Click" prefHeight="68.0" prefWidth="155.0" text="Medium">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button fx:id="hard_game" layoutX="680.0" layoutY="70.0" mnemonicParsing="false" onAction="#Hard_Game_Click" prefHeight="68.0" prefWidth="155.0" text="Hard">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button fx:id="custom_game" layoutX="80.0" layoutY="195.0" mnemonicParsing="false" onAction="#Custom_Game_Click" prefHeight="68.0" prefWidth="155.0" text="Custom Game">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button fx:id="wide_game" layoutX="380.0" layoutY="195.0" mnemonicParsing="false" onAction="#Wide_Game_Click" prefHeight="68.0" prefWidth="155.0" text="Wide">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button fx:id="invisible_game" layoutX="680.0" layoutY="195.0" mnemonicParsing="false" onAction="#Invisible_Button_Click" prefHeight="68.0" prefWidth="155.0" text="Invisible">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button fx:id="load_game" layoutX="380.0" layoutY="320.0" mnemonicParsing="false" onAction="#Load_Game_Click" prefHeight="68.0" prefWidth="155.0" text="Load Maze">
         <font>
            <Font size="20.0" />
         </font></Button>
      <VBox fx:id="menu_bar" prefHeight="30.0" prefWidth="900.0">
         <children>
            <MenuBar prefHeight="30.0" prefWidth="900.0">
               <menus>
                  <Menu mnemonicParsing="false" text="File">
                     <items>
                        <MenuItem disable="true" mnemonicParsing="false" text="New" />
                        <MenuItem disable="true" mnemonicParsing="false" text="Save" />
                        <MenuItem mnemonicParsing="false" onAction="#Load_Game_From_Menu_Bar_Click" text="Load" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Options">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#Properties_Click" text="Properties" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="More">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#About_Click" text="About" />
                        <MenuItem mnemonicParsing="false" onAction="#Help_Click" text="Help" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Exit">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#close_button_click" text="Exit Game" />
                     </items></Menu>
               </menus>
            </MenuBar>
         </children>
      </VBox>
      <Button fx:id="Maccabi_game" layoutX="80.0" layoutY="320.0" mnemonicParsing="false" onAction="#Maccabi_Button_Click" prefHeight="68.0" prefWidth="155.0" text="Maccabi Haifa Edition" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="8.0" />
         </font>
      </Button>
   </children>
</Pane>
